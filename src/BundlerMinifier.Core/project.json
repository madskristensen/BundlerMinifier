{
  "version": "1.0.0-*",
  "description": "Bundles and minifies CSS, JS and HTML files",
  "packOptions": {
    "owners": [ "Mads Kristensen" ],
    "iconUrl": "https://raw.githubusercontent.com/madskristensen/BundlerMinifier/master/src/BundlerMinifierVsix/Resources/icon.png",
    "tags": [ "concat", "bundle", "minify", "minification", "css", "js", "html" ],
    "licenseUrl": "https://github.com/madskristensen/BundlerMinifier/blob/master/LICENSE",
    "requireLicenseAcceptance": false,
    "repository": {
      "type": "git",
      "url": "https://github.com/madskristensen/BundlerMinifier"
    },
    "packageType": "DotnetCliTool"
  },
  "buildOptions": {
    "emitEntryPoint": true,
    "outputName": "dotnet-bundle",
    "define": [ "DOTNET" ]
  },

  "dependencies": {
    "NUglify": "1.5.1",
    "Newtonsoft.Json": "9.0.1"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      }
    },
    "net452": {}
  }
}
